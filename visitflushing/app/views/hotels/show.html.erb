<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @hotel.name %>
</p>

<p>
  <strong>Description:</strong>
  <%= @hotel.description %>
</p>

<p>
  <strong>Reservationurl:</strong>
  <%= @hotel.reservationurl %>
</p>

<p>
  <strong>Imageurl:</strong>
  <%= @hotel.imageurl %>
</p>
<img src="<%= @hotel.imageurl %>" atl="<%= @hotel.name %>" width="500" height="333">
<br>

<p>
  <strong>Videourl:</strong>
  <%= @hotel.videourl %>
  
</p>
<iframe width="560" height="315" src="<%= @hotel.videourl %>" 
title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; 
encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>

 
<p>
  <strong>Address:</strong>
  <%= @hotel.address %>
</p>

<p>
  <strong>Geocode:</strong>
  <%= @hotel.geocode %>
</p>


  
<%= link_to 'Edit', edit_hotel_path(@hotel) %> |
<%= link_to 'Back', hotels_path %>
